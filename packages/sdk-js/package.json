{
  "name": "@flagline/js",
  "version": "0.0.1",
  "description": "Flagline feature flag SDK for JavaScript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint --ext .ts src/",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {},
  "devDependencies": {
    "@flagline/config-eslint": "workspace:*",
    "@flagline/config-typescript": "workspace:*",
    "@flagline/types": "workspace:*",
    "eslint": "^8.57.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "license": "MIT"
}
